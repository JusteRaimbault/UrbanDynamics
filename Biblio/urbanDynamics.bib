@inbook{Birkin2013,
	Abstract = {This chapter describes a dynamic spatial microsimulation model which is applicable for small geographical areas across Great Britain. The model is capable of providing insights into not only the demographic, social and economic structure of areas but also the behaviour patterns of residents. The data, software and computational requirements of this project are discussed in some detail. We argue that the underlying procedures are sophisticated, computationally demanding and intellectually challenging but that the rewards are significant. The value of the resulting applications is illustrated with reference to a range of substantive domains, ranging from health services, transportation and crime to requirements for housing and other physical infrastructure. Some major elements of an ongoing agenda for research into the enhancement of dynamic spatial MSM are suggested. More progress is needed in the assessment and interpretation of model results, which means better techniques for model validation and more work on the social processes through which model users may be engaged. It is also argued that greater value can be extracted from a combination of more abundant data, and the refinement of modelling methods, in respect of which the possibilities for the incorporation of agent-based models, is particularly noted.},
	Address = {Dordrecht},
	Author = {Birkin, Mark},
	Booktitle = {Spatial Microsimulation: A Reference Guide for Users},
	Doi = {10.1007/978-94-007-4623-7_14},
	Editor = {Tanton, Robert and Edwards, Kimberley},
	File = {Docs/Birkin2013_Chapter_ChallengesForSpatialDynamicMic.pdf},
	Isbn = {978-94-007-4623-7},
	Pages = {223--245},
	Publisher = {Springer Netherlands},
	Title = {Challenges for Spatial Dynamic Microsimulation Modelling},
	Url = {https://doi.org/10.1007/978-94-007-4623-7_14},
	Year = {2013},
	Bdsk-Url-1 = {https://doi.org/10.1007/978-94-007-4623-7_14},
	Bdsk-Url-2 = {http://dx.doi.org/10.1007/978-94-007-4623-7_14}}

@inbook{Wu2013,
	Abstract = {People's movements, interactions and behaviours have always been at the centre of the modelling of complex social systems. This chapter introduces an individual-based, dynamic spatial microsimulation model that simulates the population evolution through the individual changes in various demographic processes at a fine spatial scale. Following discussions of the features of this model, details of the model design and development are described. The simulated results over the period of 30 years are also analysed and discussed.},
	Address = {Dordrecht},
	Author = {Wu, Belinda and Birkin, Mark},
